{
  "name": "electron-vue3-ts-image-compress",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "electron:serve": "vue-cli-service electron:serve",
    "electron:build": "vue-cli-service electron:build",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "build-icon": "electron-icon-builder --input=./public/icon.png --output=build-assets --flatten",
    "commit": "cz",
    "clean": "rimraf node_modules &&  yarn cache clean",
    "prettier-format": "prettier --write  **/*.{scss,css} **/*.{vue,ts,ts}",
    "eslint-fix": "eslint  src/* --ext .vue --ext .ts --fix",
    "prepare-husky": "yarn husky install",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "main": "background.js",
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@vueuse/core": "^8.6.0",
    "axios": "^0.27.2",
    "core-js": "^3.8.3",
    "dayjs": "^1.11.3",
    "@element-plus/icons-vue": "^2.0.5",
    "element-plus": "^2.2.4",
    "normalize.css": "^8.0.1",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "electron": "^19.0.3",
    "electron-devtools-installer": "^3.1.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-vue": "^9.1.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.7",
    "sass-loader": "^13.0.0",
    "ts-loader": "8.2.0",
    "typescript": "~4.7.3",
    "unplugin-auto-import": "^0.8.7",
    "unplugin-vue-components": "^0.19.6",
    "vue-cli-plugin-electron-builder": "~2.1.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "vue-cli-service lint",
    "*.{css,scss}": [
      "yarn prettier-format",
      "git add ."
    ],
    "*.ts?(x)": [
      "yarn prettier-format",
      "eslint --fix",
      "git add ."
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "volta": {
    "node": "16.14.1",
    "yarn": "1.22.18"
  }
}
